import java.util.Scanner;

public class Proyec11 {
    public static void main(String[] args) {
        System.out.println("Ingresa tu número favorito");
       try (Scanner sc = new Scanner(System.in)) {
        int x = sc.nextInt();
           System.out.println("TU NÚMERO FAVORITO ES : "+ x);
    }
   }

 
}

